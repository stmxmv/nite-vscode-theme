	"semanticHighlighting": true,
  	"semanticTokenColors": {
		"parameter": "#383838",
            "class": {
                "foreground": "#31565b"
            },
            "enum": "#31565b",
            "function": {
                "foreground": "#387cac"
            },
            "typeParameter": {
                "foreground": "#7953b2"
            },
            "variable": "#383838",
            "macro": {
                "foreground": "#79543b",
                "fontStyle": ""
            },
            "type": {
                "foreground": "#7953b2",
                "fontStyle": ""
            },
            "method": {
                "foreground": "#4f7f86"
            },
            "namespace": {
                "foreground": "#7953b2",
                "fontStyle": ""
            },
            "*.static": {
                "foreground": "#7953b2",
                "fontStyle": ""
            },
            "enumMember" : "#367bab",
            "property": {
                "foreground": "#367bab"
            }
	}
}