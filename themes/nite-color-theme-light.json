			"settings": {
				"foreground": "#57606A"
			}
		},
		{
			"scope": "brackethighlighter.unmatched",
			"settings": {
				"foreground": "#82071E"
			}
		},
		{
			"scope": [
				"constant.other.reference.link",
				"string.other.link"
