				"storage.modifier",
				"keyword.control", 
			 	"keyword.other.operator.overload", 
				"keyword.other.using", 
				"constant.language.false", 
				"constant.language.true"
			],
			"settings": {
				"foreground": "#A0479F",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "string, punctuation.definition.string",
			"settings": {
				"foreground": "#C0312A",
				"fontStyle": ""
			}
		},
		{
			"scope": "keyword.operator, punctuation.terminator.statement, constant.character.escape, variable",
			"settings": {
				"foreground": "#383838",
				"fontStyle": ""
